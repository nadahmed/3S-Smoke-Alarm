    "license": "MIT",
    "devDependencies": {
        "electron-packager": "^14.2.1",
        "tslint": "^5.19.0",
        "typescript": "^3.6.0",
        "electron": "^6.1.7"
    },
    "dependencies": {
        "@babel/plugin-transform-modules-commonjs": "^7.8.3",

        "electron-store": "^5.1.0",
        "mqtt": "^3.0.0",
        "play-sound": "^1.1.3"
    }
}
