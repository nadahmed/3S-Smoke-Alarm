{
    "productName": "3S Smoke Alarm",
    "name": "sssmokealarm",
    "version": "1.1.0",
    "description": "URSALINK MQTT Smoke alarm app",
    "main": "dist/main.js",
    "scripts": {
        "clean": "rimraf ./dist",
        "build": "npm run clean && tsc",
        "build-bin": "npm run build && rimraf ./releases && electron-packager . --platform=win32 --arch=x64 --overwrite --out=./releases --asar.unpackDir='player'",
        "watch": "tsc -w",
        "lint": "tslint -c tslint.json -p tsconfig.json",
        "prestart": "npm run build",
        "start": "electron ./dist/main.js",
        "pretest": "tsc -t es5 ./src/test.ts",
        "test": "node ./src/test.js"
    },
    "repository": "https://github.com/electron/electron-quick-start-typescript",
    "keywords": [
        "Electron",
        "URSALINK",
        "U87",
        "Smoke Alarm",
        "mqtt"
    ],
    "author": "Hivecore.io",
    "license": "MIT",
    "devDependencies": {
        "electron": "^6.1.12",
        "electron-packager": "^14.2.1",
        "rimraf": "^3.0.2",
        "tslint": "^5.19.0",
        "typescript": "^3.6.0"
    },
    "dependencies": {
        "@babel/plugin-transform-modules-commonjs": "^7.8.3",
        "@types/nedb": "^1.8.9",
        "electron-db": "^1.0.0",
        "electron-store": "^5.1.0",
        "electron-util": "^0.13.1",
        "mqtt": "^3.0.0",
        "nedb": "^1.8.0",
        "node-mp3-player": "^1.0.3",
        "play-sound": "^1.1.3",
        "rxjs": "^6.5.5"
    }
}
